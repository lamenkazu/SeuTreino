<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/exerciseListingFragment">

    <fragment
        android:id="@+id/exerciseListingFragment"
        android:name="com.example.seutreino.exercise.ExerciseListingFragment"
        android:label="fragment_exercise_listing"
        tools:layout="@layout/fragment_exercise_listing" >
        <action
            android:id="@+id/action_exerciseListingFragment_to_exerciseDetailFragment"
            app:destination="@id/exerciseDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/exerciseDetailFragment"
        android:name="com.example.seutreino.exercise.ExerciseDetailFragment"
        android:label="fragment_exercise_detail"
        tools:layout="@layout/fragment_exercise_detail" />


    <fragment
        android:id="@+id/workoutListingFragment"
        android:name="com.example.seutreino.workout.WorkoutListingFragment"
        android:label="fragment_workout_listing"
        tools:layout="@layout/fragment_workout_listing" >
        <action
            android:id="@+id/action_workoutListingFragment_to_workoutDetailFragment"
            app:destination="@id/workoutDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/workoutDetailFragment"
        android:name="com.example.seutreino.workout.WorkoutDetailFragment"
        android:label="fragment_workout_detail"
        tools:layout="@layout/fragment_workout_detail" />
</navigation>